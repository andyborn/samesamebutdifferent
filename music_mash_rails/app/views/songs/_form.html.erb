<%= form_for @song do |f| %>
  
  <p>
    <%= f.label :artist_name %><br/>
    <%= f.text_field :artist_name %>
  </p>
  <p>
    <%= f.label :song_name %><br/>
    <%= f.text_field :song_name%>
  </p>
   <p>
    <%= f.label :deezer_url %><br/>
    <%= f.text_field :deezer_url%>
  </p>
  <p><%= f.submit %></p>
<% end %>
