<div id="tape_container">
  <div id="tape">
<%= form_for @song do |f| %>
  
    
    
      
      <%= image_tag "cassette_tape_red.png", class: "tape_image" %> 
      
      <div id="cassette_cog1"><%= image_tag "cassette_cog.png", id: "cog1"%></div>
      <div id="cassette_cog2"><%= image_tag "cassette_cog.png", id: "cog2"%></div>
      <div id="song_input"> 
        <%= f.text_field :song_name, placeholder: "Enter track name", autofocus: true, autocomplete: "off"%> 
      </div>
      <div id="artist_input">
        <%= f.text_field :artist_name, placeholder: "Enter artist name", autocomplete: "off" %>
      </div>
      <div id="deezer_url">
        <%= f.hidden_field :deezer_url, placeholder: "deezer url" %>
      </div>  
      
      <div id="song_submit">
        <%= f.submit "Get Songs!" %>
      </div>


  
<% end %>
  
   

      <div id="side_A_button"><button>A&lt;&lt;</button></div>
      <div id="side_B_button"><button>&gt;&gt;B</button></div>
      <div id="eject_link"><%= link_to image_tag("eject_icon.png"), destroy_user_session_path, method: :delete %>
      </div>
 </div> 
</div>  